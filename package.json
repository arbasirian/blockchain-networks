{
  "name": "blockchain-networks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel scss:watch next:dev",
    "scss:watch": "yarn typed-scss-modules src --updateStaleOnly -L error --nameFormat none -w --implementation sass --exportType default --exportTypeInterface IStyles -i src/styles/design-system --aliases.variables src/styles/design-system/variables --aliases.mixins src/styles/design-system/mixins",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@artsy/fresnel": "^3.5.0",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.27.2",
    "next": "12.1.6",
    "next-redux-wrapper": "^7.0.5",
    "next-with-less": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "sass": "^1.51.0",
    "typed-scss-modules": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "babel-loader": "^8.2.5",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-storybook": "^0.5.7",
    "postcss": "^8",
    "sass-loader": "^12.6.0",
    "storybook-addon-sass-postcss": "^0.1.0",
    "typescript": "4.6.4"
  }
}
